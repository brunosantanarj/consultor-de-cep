{
  "defaultSeverity": "error",
  "extends": ["tslint-react-recommended",  "tslint-react-a11y"],
  "jsRules": {
  },
  "rules": {
      "indent": [true, "spaces", 2],
      "ter-indent": [true, 2],
      "quotemark": [true, "single", "jsx-single"],
      "no-submodule-imports": false,
      "space-before-function-paren": false,
      "no-implicit-dependencies": false,
      "no-empty": true,
      "jsx-curly-spacing": "never",
      "trailing-comma": [true, {
          "multiline": {
              "objects": "always",
              "arrays": "always",
              "functions": "never",
              "typeLiterals": "ignore"
          },
          "singleline": "never"
      }],
      "no-irregular-whitespace": true,
      "no-empty-interface": true,
      "no-unnecessary-type-assertion": true,
      "arrow-return-shorthand": [true, "multiline"],
      "max-file-line-count": [true, 160],
      "max-line-length": [true, 120],
      "no-console": true,
      "import-spacing": true,
      "interface-name": [true, "always-prefix"]
  },
  "linterOptions": {
      "exclude": [
          "./coverage",
          "./postcss.config.js",
          "./jest.config.js",
          "./jest.setup.js",
          "./next.config.js"
      ]
  },
  "rulesDirectory": []
}